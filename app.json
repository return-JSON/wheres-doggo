{
  "expo": {
    "name": "Where's DogGo?",
    "slug": "wheresdoggo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/_Logo.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/dogpic.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Where's DogGo is a mobile dog-scouting game which uses your mobile device GPS to locate and capture virtual dogs, which appear as if they were in your real-time location.",
    "githubUrl": "https://github.com/return-JSON/wheres-doggo/"
  }
}
